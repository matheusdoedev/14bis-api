{
  "name": "14bis-api",
  "version": "1.0.0",
  "main": "src/server/server.ts",
  "author": "Matheusdoe-dev <matheusdoe.dev@gmail.com>",
  "scripts": {
    "prestart": "NODE_ENV=dev sequelize db:create",
    "start": "NODE_ENV=production yarn build && node build/server/server.js",
    "build": "tsc --outDir build",
    "predev": "NODE_ENV=dev sequelize db:create",
    "dev": "NODE_ENV=dev ts-node-dev --transpile-only --ignore-watch node_modules --respawn src/server/server.ts",
    "pretest": "NODE_ENV=test sequelize db:migrate:undo:all && rm --force ./tests/database.sqlite && NODE_ENV=test sequelize db:migrate",
    "test": "NODE_ENV=test jest --coverage",
    "posttest": "NODE_ENV=test sequelize db:migrate:undo:all && rm --force ./tests/database.sqlite"
  },
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.2.5",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0",
    "typescript": "^4.0.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.8",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.9",
    "@types/factory-girl": "^5.0.2",
    "@types/faker": "^5.1.5",
    "@types/jest": "^26.0.15",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/sequelize": "^4.28.9",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "eslint": "^7.15.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.2.0",
    "factory-girl": "^5.0.4",
    "faker": "^5.1.0",
    "jest": "^26.6.3",
    "sqlite3": "^5.0.0",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0"
  }
}
